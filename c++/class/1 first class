//first class 

#include<bits/stdc++.h>
using namespace std;
class point        // after appling public : we removed error  now members are accessible 
{     public:       //error occurs due to being private now 
    int x;            // else members are private by default
    int y;
};                       // output 10 20
int main()
{ point p;
p.x= 10;
p.y=20;
cout<<p.x<<" "<<p.y;
    return 0;
}
