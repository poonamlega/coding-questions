//Print a given string upto the given times


#include<iostream>
using namespace std;
int main()
{
    int i,n;
    char str[10];
    cout<<"Enter the string"<<endl;
    cin>> str;
    cout<<"Enter the number of times you want to repeat the string"<<endl;
    cin>>n;
    cout<< "Repeated string is"<<endl;
   for(i = 0 ;i <= n ;i++)
   cout << i <<" "<<str<<endl;
    return 0;
    
    
}
